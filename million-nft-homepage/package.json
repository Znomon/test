{
  "name": "million-nft-homepage",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "clean": "rimraf dist",
    "watch-lib:sc-crypto": "ng build sc-crypto --watch",
    "watch-lib:common": "wait-on dist/sc-crypto/bundles/sc-crypto.umd.js && ng build common --watch",
    "serve:app": "wait-on dist/common/bundles/common.umd.js && ng serve",
    "dev-build": "npm run clean && run-p watch-lib:* serve:app",
    "prod-lib-build": "ng build shared",
    "prod-build": "npm run clean && run-p prod-lib-build && node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.0",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "~9.0.0",
    "@angular/compiler": "~9.0.0",
    "@angular/core": "~9.0.0",
    "@angular/forms": "~9.0.0",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "~9.0.0",
    "@angular/platform-browser-dynamic": "~9.0.0",
    "@angular/router": "~9.0.0",
    "@truffle/contract": "^4.3.13",
    "ipfs-http-client": "^50.1.0",
    "npm-run-all": "^4.1.5",
    "tslib": "^1.10.0",
    "wait-on": "^5.3.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^11.1.1",
    "@angular-devkit/build-angular": "~0.900.7",
    "@angular-devkit/build-ng-packagr": "~0.900.7",
    "@angular/cli": "~9.0.0",
    "@angular/compiler-cli": "~9.0.0",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/language-service": "~9.0.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.20.8",
    "@types/react": "^17.0.3",
    "@walletconnect/web3-provider": "^1.4.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^9.0.0",
    "protractor": "~5.4.3",
    "rxjs": "^6.5.5",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "~3.7.5",
    "web3": "^1.2.11",
    "web3modal": "^1.9.3"
  },
  "browser": {
    "http": false,
    "https": false,
    "net": false,
    "path": false,
    "stream": false,
    "tls": false,
    "crypto": false,
    "os": false
  }
}
